function calculator(num1, operator, num2) {

    let myMap = {
        '+': (a, b) => a + b,
        '-': (a, b) => a - b,
        '*': (a, b) => a * b,
        '/': (a, b) => a / b,
    }

    let endResult = myMap[operator](num1, num2)
    console.log(endResult.toFixed(2))
}

calculator(5, '+', 10)
calculator(25.5, '-', 3)
