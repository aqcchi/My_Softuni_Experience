command = input()
coffee_counter = 0

while command != "END":

    if command == "coding":
        coffee_counter += 1
    elif command == "CODING":
        coffee_counter += 2
    elif command == "cat" or command == "dog":
        coffee_counter += 1
    elif command == "CAT" or command == "DOG":
        coffee_counter += 2
    elif command == "movie":
        coffee_counter += 1
    elif command == "MOVIE":
        coffee_counter += 2
    else:
        pass

    command = input()

if coffee_counter > 5:
    print("You need extra sleep")
else:
    print(coffee_counter)
