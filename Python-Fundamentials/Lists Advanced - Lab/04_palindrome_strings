words = input().split()
palindrome = input()
list_of_palindromes = []

for word in words:
    if word == word[::-1]:
        list_of_palindromes.append(word)

count_of_palindrome = list_of_palindromes.count(palindrome)

print(list_of_palindromes)
print(f"Found palindrome {count_of_palindrome} times")
